<?xml version="1.0"?>
<launch>
	<node name="navsat_trasnform_node" pkg="robot_localization" type="navsat_trasnform_node" clear_params="true" output="screen">
		<!-- load yaml file -->
		<rosparam command="load" file="$(find soma_samples)/launch/config/navsat_transform_template.yaml" />
		<!--  -->
		<param name="magnetic_declination_radians" value="0"/>
		<param name="yaw_offset" value="0"/>

		<remap from="/odometry/filtered" to="/gps_odom" />
		<remap from="/gps/fix" to="/fix" />
		<remap from="imu_data" to="/imu/data" />
	</node>

	<node name="ekf_gyrodometry" pkg="robot_localization" type="ekf_localization_node" clear_params="true" output="screen">
		<!-- load yaml file -->
		<rosparam command="load" file="$(find soma_samples)/launch/config/ekf_gpsimulidar.yaml" />
		<!--  -->
		<remap from="/odometry/filtered" to="/combined_odom" />
		<remap from="odom0" to="/lidar_odom" />
		<remap from="odom1" to="/gps_odom" />
		<remap from="imu_data" to="/imu/data" />
	</node>
</launch>
